{
    "compilerOptions": {
        "allowJs": true,
        "allowSyntheticDefaultImports": true,
        "esModuleInterop": true,
        "isolatedModules": true,
        "jsx": "react",
        "lib": [
            "es6"
        ],
        "moduleResolution": "node",
        "noEmit": true,
        "strict": true,
        "target": "esnext"
    },
    "exclude": [
        "node_modules",
        "native-base-theme",
        "native-base-theme/components",
        "native-base-theme/variables",
        "babel.config.js",
        "metro.config.js",
        "jest.config.js"
    ]
}